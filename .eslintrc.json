{
  "extends": [
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "./node_modules/gts/"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "node/no-unpublished-import": "off",
    "prefer-template": "error"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["@emotion"]
}
